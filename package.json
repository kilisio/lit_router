{
  "name": "lit_router",
  "version": "1.2.6",
  "main": "lit-element-router.js",
  "scripts": {
    "rls": "standard-version",
    "commit": "git-cz",
    "build": "npm run build_lib",
    "build_app": "rollup -c ./config/rollup/app_build.config.js",
    "build_lib": "rollup -c ./config/rollup/lib_build.config.js",
    "pub_lib": "cd $INIT_CWD/build/lib && npm publish",
    "serve_demo": "serve app"
  },
  "keywords": [
    "lit",
    "element",
    "router",
    "lit-element",
    "lit-element-router",
    "regexp"
  ],
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "absurd": "^0.3.9",
    "cheerio": "^1.0.0-rc.3",
    "clean-css": "^4.2.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "esm": "^3.1.4",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "html-minifier": "^4.0.0",
    "husky": "^1.3.1",
    "jshint": "^2.9.7",
    "lit-element": "^2.0.1",
    "open": "^6.4.0",
    "prettier": "^1.16.1",
    "rollup": "^1.10.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-generate-package-json": "^3.1.3",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "serve": "^11.2.0",
    "standard-version": "^4.4.0"
  },
  "dependencies": {
    "lit-element": "^2.0.1"
  },
  "nyc": {
    "temp-dir": "./node_modules/.cache/alternative-tmp"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "maxHeaderWidth": 200,
      "maxLineWidth": 200
    }
  },
  "husky": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }
}
